<!DOCTYPE html>
<html>
<head>
<title>SubbyDevise</title>
<%= stylesheet_link_tag 'reset', 'text', '960', 'application' %>
<%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.1/jquery.min.js" %>
<%= javascript_include_tag 'rails' %>
<%= csrf_meta_tag %>
</head>
<body>
  <header>
    <% if current_user %>
      welcome <%= current_user.username %> <%= link_to 'logout', logout_url, :method => :delete %>
    <% else %>
      <%= link_to 'login', login_url %>
    <% end %>
  </header>
  
  <section>
    <aside id="sidebar">
      <h2>Users</h2>
      <p>...</p>
      <h2>Foos</h2>
      <p>...</p>
      <h2>Bars</h2>
      <p>...</p>
    </aside>
    <article>
      <%= yield %>
    </article>
  </section>
</body>
</html>
